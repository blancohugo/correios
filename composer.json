{
    "name": "blancohugo/correios",
    "description": "Consulta de cep no SIGEP dos Correios com PHP",
    "type": "library",
    "keywords": [
        "blancohugo",
        "correios",
        "correios-php",
        "cep",
        "php"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Hugo Blanco",
            "email": "blancohfb@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "BlancoHugo\\Correios\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BlancoHugo\\Correios\\": "tests/src"
        }
    },
    "scripts": {
        "test": "phpunit --testdox --colors=always",
        "check": "phpcs -p --standard=phpcs.xml"
    },
    "config": {
        "sort-packages": true
    }
}
